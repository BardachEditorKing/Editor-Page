; Function to display a random message
RandomMessage() {
    global messages
    Random, randIndex, 1, % messages.MaxIndex()
    Send, % messages[randIndex]
    Send, {Enter}
}

; Function to generate a random delay time (in milliseconds)
RandomDelay(minTime, maxTime) {
    Random, randTime, %minTime%, %maxTime%
    return randTime
}

; Main loop to display random messages at random intervals
Loop {
    RandomMessage()
    Sleep, % RandomDelay(10000, 20000) ; Random delay between 10 and 20 seconds
}

; Hotkey to start the loop
^s:: ; Ctrl+S hotkey
SetTimer, MessageLoop, On
return

MessageLoop:
    RandomMessage()
    SetTimer, MessageLoop, % RandomDelay(5000, 15000)
return

; Hotkey to stop the loop
^e:: ; Ctrl+E hotkey
SetTimer, MessageLoop, Off
return

; Toggle pause with the F key
F2::Pause, Toggle
